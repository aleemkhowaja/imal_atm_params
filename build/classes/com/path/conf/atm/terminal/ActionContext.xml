<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="terminalListAction"
		class="com.path.atm.actions.terminal.TerminalListAction"
		scope="prototype" parent="baseAction">
		<property name="terminalBO" ref="terminalBO"></property>
	</bean>

	<bean id="terminalMaintAction"
		class="com.path.atm.actions.terminal.TerminalMaintAction"
		scope="prototype" parent="baseAction">
		<property name="terminalBO" ref="terminalBO"></property>
	</bean>


</beans>