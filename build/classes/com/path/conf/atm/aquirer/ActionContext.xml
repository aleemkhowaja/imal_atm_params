<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="acquirerListAction"
		class="com.path.atm.actions.acquirer.AcquirerListAction"
		scope="prototype" parent="baseAction">
		<property name="acquirerBO" ref="acquirerBO"></property>
	</bean>

	<bean id="acquirerMaintAction"
		class="com.path.atm.actions.acquirer.AcquirerMaintAction"
		scope="prototype" parent="baseAction">
		<property name="acquirerBO" ref="acquirerBO"></property>
	</bean>
</beans>